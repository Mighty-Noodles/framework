{
  "name": "express_api_starter",
  "version": "0.0.1",
  "description": "My app description",
  "main": "src/index.ts",
  "scripts": {
    "ts-node-path": "ts-node -r tsconfig-paths/register",
    "dev:start": "NODE_ENV=production nodemon",
    "dev": "NODE_ENV=development nodemon",
    "test": "jest --runInBand --forceExit --detectOpenHandles",
    "test:watch": "yarn test --watch",
    "test:setup": "yarn ts-node-path ./scripts/test.setup.ts",
    "db:migrate": "node-pg-migrate create",
    "db:migrate:up": "yarn ts-node-path ./scripts/migration/up.ts",
    "db:migrate:down": "yarn ts-node-path ./scripts/migration/down.ts",
    "build:server": "rm -rf ./build-server && tsc --outDir build-server",
    "build:sdk": "browserify lib/auth/sdk/auth.sdk.ts -p [ tsify --transpileOnly ] > public/sdk/auth.sdk.js",
    "build": "yarn build:server && yarn build:sdk",
    "start:prod": "DEBUG=app:* node ./server-build/src/index.js"
  },
  "engines": {
    "node": "12.18.4"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.8",
    "@types/debug": "^4.1.5",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.8",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.163",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.14.5",
    "aws-sdk": "^2.784.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "browserify": "^17.0.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "knex": "^0.21.9",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "node-fetch": "^2.6.1",
    "node-pg-migrate": "^5.8.1",
    "nodemailer": "^6.4.14",
    "objection": "^2.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsify": "^5.0.2",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-jest": "^26.6.1",
    "eslint": "^7.12.1",
    "eslint-plugin-jest": "^24.1.0",
    "html-loader": "^1.3.2",
    "jest": "^26.6.1",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.0",
    "ts-node-dev": "^1.0.0"
  }
}
